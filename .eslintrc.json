{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-debugger": 0,
        "no-alert": 0,
        "no-unused-vars": 1,
        "prefer-const": [
        "error",
        {
            "destructuring": "all"
        }
        ]
    }
}
